<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="enity.Voter" table="voter" schema="" catalog="votesys">
        <id name="id">
            <column name="Id" sql-type="int" not-null="true"/>
        </id>
        <property name="voteId">
            <column name="vote_id" sql-type="int"/>
        </property>
        <property name="ip">
            <column name="Ip" sql-type="varchar" length="45"/>
        </property>
    </class>
</hibernate-mapping>