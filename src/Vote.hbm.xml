<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="enity.Vote" table="vote" schema="" catalog="votesys">
        <id name="voteId">
            <column name="vote_id" sql-type="int" not-null="true"/>
        </id>
        <property name="title">
            <column name="title" sql-type="varchar" length="50"/>
        </property>
        <property name="createdate">
            <column name="createdate" sql-type="varchar" length="50"/>
        </property>
        <property name="type">
            <column name="type" sql-type="int"/>
        </property>
        <property name="publish">
            <column name="publish" sql-type="int"/>
        </property>
        <property name="adminId">
            <column name="admin_id" sql-type="int"/>
        </property>
    </class>
</hibernate-mapping>