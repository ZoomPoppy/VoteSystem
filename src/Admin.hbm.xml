<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="enity.Admin" table="admin" schema="" catalog="votesys">
        <id name="adminId">
            <column name="admin_id" sql-type="int" not-null="true"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar" length="50"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar" length="50"/>
        </property>
        <property name="logintime">
            <column name="logintime" sql-type="varchar" length="50"/>
        </property>
    </class>
</hibernate-mapping>